{
  "name": "reactube",
  "version": "1.0.0",
  "description": "A Youtube clone built with React and Express",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "aws-sdk": "^2.309.0",
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "fluent-ffmpeg": "^2.1.2",
    "knex": "^0.15.2",
    "multer": "^1.3.1",
    "pg": "^7.4.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "screenfull": "^3.3.2"
  }
}
